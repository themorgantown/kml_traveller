# Copy this file to .env and replace with your actual API keys

# Google Maps API Key (for run.py)
# Get from: https://console.cloud.google.com/
# Required APIs: Geocoding API, Distance Matrix API
GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here

# OpenRouteService API Key (for run_ors.py) 
# Get from: https://openrouteservice.org/
# Free tier: 2,000 requests/day
ORS_API_KEY=your-openrouteservice-api-key-here

# Optional Configuration
CACHE_DURATION_DAYS=30
MAX_REASONABLE_DISTANCE=200000
